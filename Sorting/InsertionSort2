public static void insertionSortPart2(int[] ar) {
        for (int i = 0; i < ar.length; i++) {
            insertionSort1(ar);
            for (int j = 0; j < i; j++) {
                insertionSort1(ar);
            }
            printArray(ar);
        }
    }



    public static void insertionSort1(int[] ar) {
        int elementToBeShiftedDown = 0;
        int elementToBeShiftedUp = 0;
        int indexOfElementToBeShiftedUp = 0;
        for (int i = 0; i < ar.length - 1; i++) {
            if (ar[i] > ar[i + 1]) {
                elementToBeShiftedDown = ar[i + 1];
                elementToBeShiftedUp = ar[i];
                ar[i] = elementToBeShiftedDown;
                ar[i + 1] = elementToBeShiftedUp;
            }
        }
    }
